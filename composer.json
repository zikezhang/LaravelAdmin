    "keywords": ["laravel", "admin", "backend", "sentry", "cartalyst"],
    "homepage": "https://github.com/zikezhang/LaravelAdmin",
    "license": "MIT",
    "authors": [
        {
            "name": "ZZK",
            "email": "zhzike@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~4",
        "cartalyst/sentry": "2.1.*",
        "anahkiasen/former": "3.5.*"
